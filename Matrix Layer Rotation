#include<bits/stdc++.h>
 
using namespace std;
 
typedef long long ll;
 
ll mat[345][345];
int n,m;
ll q;
  
  
void __rotate(){
    int i1,i2,i3,i4;
    int j1,j2,j3,j4;
    ll v1,v2,v3,v4;
    for(int i = min(m , n ) / 2 - 1; i >= 0; i-- ){
        ll x = q % (((m-2*i) + (n-2*i)) * 2 - 4);
        while(x--){
        // 4-corner points
        i1 = i,         j1 = i;
        i2 = i,         j2 = m-i-1;
        i3 = n-i-1,     j3 = i;
        i4 = n-i-1,     j4 = m-i-1;
         
        //value of 4-corner points
        v1 = mat[i1][j1];
        v2 = mat[i2][j2];
        v3 = mat[i3][j3];
        v4 = mat[i4][j4];
         
        for(int j = j1 + 1 ; j <= j2 ; j++ ){
            swap(mat[j1][j],mat[j1][j-1]);
        }
        for(int j = i3 - 1; j > i1 ; j--){
            swap(mat[j][j1], mat[j+1][j1]);
        }
        for(int j = j4 - 1 ; j > j3 ; j--){
            swap(mat[i3][j] , mat[i3][j+1]);
        }
        for(int j = i2+1; j < i4; j++){
            swap(mat[j][j2], mat[j-1][j2]);
        }
        mat[i1+1][j1] = v1;
        mat[i3][j3+1] = v3;
        mat[i4-1][j4] = v4;
        }
    } 
}  
  
int main(){
    cin>>n>>m>>q;
    for(int i = 0; i < n ; i++){
        for(int j = 0 ; j < m ; j++){
            cin>>mat[i][j];
        }
    }
 
    __rotate();
     
    for(int i = 0; i < n; i++){
        for(int j = 0 ; j < m ; j ++){
            cout<<mat[i][j]<<" ";
        }cout<<endl;
    }
} 
