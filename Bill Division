#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() 
{ 
    int n,k,x=0,p,a;
    cin>>n>>k;
    for(int i=0;i<n; i++)
    {
        cin>>a;
        if(i!=k)
            x+=a;
    }
    x=x/2;
    cin>>p;
    if(p-x==0)
        cout<<"Bon Appetit";
    else cout<<p-x;
    return 0;
}
